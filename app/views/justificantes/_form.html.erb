<%= simple_form_for(@justificante, html: { class: 'form-horizontal' } ) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :identificador %>
    <%= f.input :matricula %>
    <%= f.input :bastidor %>
    <%= f.input :comprador, as: 'string' %>
    <%= f.input :vendedor, as: 'string' %>
    <%= f.input :marca %>
    <%= f.input :modelo %>
    <%= f.input :fecha_alta %>
    <%= f.input :fecha_entra_trafico %>
    <%= f.input :fecha_sale_trafico %>
    <%= f.input :dias_tramite %>
    <%= f.input :matriculacion, label: "¿Nueva matriculación?" %>
    <%= f.input :status, label: t("Subir PDF") %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
