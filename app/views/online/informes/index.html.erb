<% provide :title, t("Listado de Informes de Trafico") %>

<div class="about_page">
  <%= section_header t("Informes de trafico") %>
  <div class="row pull-right">
    <%= link_to 'Nuevo Informe', new_online_informe_path, class: 'btn btn-primary new_record' %>
  </div>
  <div class="row">
    <table id="informes" class="row pull-right table table-bordered table-striped table-condensed informe_traficos top_filtered">
      <thead>
        <tr class="filter">
          <th>Organización</th>
          <th>Matrícula</th>
          <th>Solicitante</th>
          <th>Fecha solicitud</th>
          <th>Finalizado</th>
          <th></th>
          <th></th>
        </tr>
        <tr class="titles">
          <th>Organización</th>
          <th>Matrícula</th>
          <th>Solicitante</th>
          <th>Fecha solicitud</th>
          <th>Finalizado</th>
          <th colspan="2"></th>
        </tr>
      </thead>
      <tbody>
          <%= render informes %>
      </tbody>
    </table>

  </div>
</div>