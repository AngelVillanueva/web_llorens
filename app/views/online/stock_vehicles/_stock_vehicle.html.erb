        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
        <h3></h3>
      </div>
      <div class="modal-body">
        <div class="col1">
          <p class="pmatricula"><strong><%= t("Matricula") %></strong>: <span></span></p>
          <p class="pparticular"><strong><%= t("Particular") %></strong>: <span></span></p>
          <p class="pcomprav"><strong><%= t("Compra_venta") %></strong>: <span></span></p>
          <p class="pmarca"><strong><%= t("Marca") %></strong>: <span></span></p>
          <p class="pmodelo"><strong><%= t("Modelo") %></strong>: <span></span></p>
          <p class="pcomprador"><strong><%= t("Comprador") %></strong>: <span></span></p>
          <p class="pft"><strong><%= t("Ft") %></strong>: <span></span></p>
          <p class="ppc"><strong><%= t("Pc") %></strong>: <span></span></p>
          <p class="pitv"><strong><%= t("Fecha_itv") %></strong>: <span></span></p>
          <p class="pincidencia"><strong><%= t("Incidencia") %></strong>: <span></span></p>
        </div>
        <div class="col2">
          <p class="pfec"><strong><%= t("Fecha_expediente_completo") %></strong>: <span></span></p>
